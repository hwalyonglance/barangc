    "@angular/cdk": "^2.0.0-beta.10",
    "@angular/common": "^4.3.6",
    "@angular/compiler": "^4.3.6",
    "@angular/core": "^4.3.6",
    "@angular/flex-layout": "^2.0.0-beta.9",
    "@angular/forms": "^4.3.6",
    "@angular/http": "^4.3.6",
    "@angular/material": "^2.0.0-beta.10",
    "@angular/platform-browser": "^4.3.6",
    "@angular/platform-browser-dynamic": "^4.3.6",
    "@angular/platform-server": "^4.3.6",
    "@angular/platform-webworker": "^4.3.6",
    "@angular/platform-webworker-dynamic": "^4.3.6",
    "@angular/router": "^4.3.6",
    "@angular/service-worker": "^1.0.0-beta.6",
    "@angular/testing": "^0.0.0-0",
    "@angular/tsc-wrapped": "^4.3.6",
    "@angular/typescript": "^2.0.0-5d0e199",
    "@angular/upgrade": "^4.2.4",
    "@nguniversal/express-engine": "^1.0.0-beta.3",
    "@nguniversal/module-map-ngfactory-loader": "^1.0.0-beta.3",
    "angular-in-memory-web-api": "^0.3.2",
    "angular-ssr": "^0.10.39",
    "angular2-in-memory-web-api": "0.0.21",
    "animate.css": "^3.5.2",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.1",
    "font-awesome": "^4.7.0",
