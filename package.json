    "rollup-plugin-uglify": "^2.0.1",
    "source-map-explorer": "^1.4.0",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "typescript": "^2.5.2",
    "typings": "^2.1.1"
  }
