{
  "name": "barang",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build": "ng build --prod --aot && ng build --prod --app universal --output-hashing=none",
    "ng-render": "ng-render",
    "prebuild-aot": "ngc -p tsconfig-aot.json",
    "build-aot": "rollup -c rollup-config.js",
    "postbuild-aot": "source-map-explorer aot/build.js",
    "build:static": "ng build --prod && ng build --prod --app 1 --output-hashing=false && node prerender",
    "build:dynamic": "ng build --prod && ng build --prod --app 1 --output-hashing=false && cpy ./server.js ./dist",
    "serve:static": "npm run build:static && cd dist && http-server",
    "serve:dynamic": "npm run build:dynamic && cd dist && node server"
  },
  "private": true,
  "dependencies": {
    "@angular/app-shell": "^0.1.0",
    "@angular/animation": "^4.0.0-beta.8",
    "@angular/animations": "^4.3.6",
